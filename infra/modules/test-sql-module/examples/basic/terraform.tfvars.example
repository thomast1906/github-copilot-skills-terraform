# Example Terraform Variables Configuration
# Copy this file to terraform.tfvars and customize the values

# Resource naming
resource_group_name = "rg-sqltest-dev-eastus-001"
location            = "East US"
environment         = "dev"

# SQL Server configuration
sql_server_name    = "sql-testserver-dev-eastus-001"
sql_admin_username = "sqladmin"
# sql_admin_password = "P@ssw0rd123456!"  # DO NOT commit passwords! Use environment variable or Key Vault

# Database configuration
database_name     = "testdb"
elastic_pool_name = "pool-sqltest-dev-eastus-001"

# Network configuration
vnet_name = "vnet-sqltest-dev-eastus-001"

# Firewall configuration
office_ip_start = "203.0.113.0"    # Example IP range (TEST-NET-3)
office_ip_end   = "203.0.113.255"  # Replace with your actual office IP range
